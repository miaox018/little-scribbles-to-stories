# Maintenance Mode Configuration
# Set to 'true' to enable maintenance mode (blocks all public access)
VITE_MAINTENANCE_MODE=false

# Admin Override Configuration  
# Set to 'true' to allow admin access during maintenance mode
VITE_ADMIN_OVERRIDE=false

# Supabase Edge Function Environment Variables (for production deployment)
# Set these in your deployment environment, not in local .env files
# MAINTENANCE_MODE=false
# ADMIN_OVERRIDE=falsev